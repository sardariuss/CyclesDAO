{
  "version": 1,
  "canisters": {
    "cyclesDAO": {
      "type": "motoko",
      "main": "src/CyclesDAO/main.mo",
      "declarations": {
				"output": "src/frontend/declarations/cyclesDAO"
			}
    },
    "toPowerUp": {
      "type": "motoko",
      "main": "src/ToPowerUp/toPowerUp.mo"
    },
    "toPowerUpFactory": {
      "type": "motoko",
      "main": "src/ToPowerUp/toPowerUpFactory.mo"
    },
    "dip20": {
      "type": "custom",
      "candid": "src/DIP20/dip20.did",
      "wasm": "src/DIP20/dip20.wasm"
    },
    "ledger": {
      "type": "custom",
      "candid": "src/Ledger/ledger.did",
      "wasm": "src/Ledger/ledger.wasm"
    },
    "extf":{
      "type": "custom",
      "candid": "src/ExtFungible/extf.did",
      "wasm": "src/ExtFungible/extf.wasm"
    },
    "basicDAO": {
      "type": "custom",
      "candid": "src/BasicDAO/basicDAO.did",
      "wasm": "src/Ledger/basicDAO.wasm"
    },
    "frontend": {
      "frontend": {
        "entrypoint": "index.html"
      },
      "source": ["src/frontend/dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
